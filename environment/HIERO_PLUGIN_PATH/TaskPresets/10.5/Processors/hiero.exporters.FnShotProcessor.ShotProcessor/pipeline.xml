                    <useSingleSocket valuetype="bool">False</useSingleSocket>
                    <additionalNodesEnabled valuetype="bool">False</additionalNodesEnabled>
                    <deleteAudio valuetype="bool">True</deleteAudio>
                    <additionalNodesData valuetype="list" />
